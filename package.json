{
  "name": "multi-integer-range",
  "version": "5.2.0",
  "description": "Parses and manipulates multiple comma-separated integer ranges (eg 1-3,8-10)",
  "repository": "https://github.com/smikitky/node-multi-integer-range.git",
  "sideEffects": false,
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "c8 tsx --test src/**/*.test.ts",
    "doc": "node scripts/docgen.js",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "prepack": "npm run build",
    "prettier": "prettier --write src/**/*.{ts,js}"
  },
  "keywords": [
    "integer",
    "range",
    "page",
    "multiple",
    "parseInt",
    "parse"
  ],
  "author": "Soichiro Miki",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "c8": "^10.1.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "runkitExampleFilename": "example.js"
}